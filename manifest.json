{
    "manifest_version": 3,
    "name": "NoReview",
    "description": "Base Level Extension",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popap.html",
        "default_icon": "icons/icon.png"
    }
}